{
  "name": "tech-triage-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prisma:dev": "dotenv -e .env.prisma-dev -- npx prisma dev",
    "prisma:dev:logs": "tsx scripts/run-prisma-dev-with-logs.ts",
    "dev": "dotenv -e .env.prisma-dev -- next dev --turbopack",
    "dev:logs": "tsx scripts/run-next-dev-with-logs.ts",
    "dev:classic": "dotenv -e .env -- next dev",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "export-forms": "tsx scripts/export-forms.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "studio": "tsx scripts/run-prisma-studio-dev.ts",
    "export:prod": "dotenv -e .env.export -- npm run export-forms -- --destination blob",
    "catalog:validate": "tsx scripts/catalog/validate-binding-paths.ts",
    "studio:azure": "tsx scripts/run-prisma-studio-azure.ts",
    "db:seed": "tsx prisma/seed/index.ts",
    "db:seed:dev": "dotenv -e .env.prisma-dev -- tsx prisma/seed/index.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed/index.ts"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.24.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-pdf/renderer": "^4.3.1",
    "exceljs": "^4.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "luxon": "^3.5.0",
    "next": "15.5.3",
    "next-themes": "^0.4.6",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/luxon": "^3.3.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
